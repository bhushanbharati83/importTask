{
"name": "vendor/laravel-user-discounts",
"description": "User-level discounts with deterministic stacking for Laravel",
"type": "library",
"license": "MIT",
"autoload": {
    "psr-4": {
        "Vendor\\UserDiscounts\\": "src/"
    }
},
"require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
},
"require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
},
"extra": {
    "laravel": {
        "providers": [
            "Vendor\\UserDiscounts\\DiscountsServiceProvider"
        ]
    }
},
"minimum-stability": "stable",

}